          "datasource": { "type": "loki", "uid": "${datasource}" },
          "expr": "sum by (level) (count_over_time({job=~\".+\", service_name=~\"$service\"} | json | level=~\"$level\" [$__interval]))",
          "legendFormat": "{{level}}",
          "refId": "A"
        }
      ],
      "title": "Log Volume by Level",
      "type": "timeseries"
